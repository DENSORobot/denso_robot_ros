<?xml version="1.0"?>
<package>
  <name>denso_robot_ros</name>
  <version>3.0.2</version>
  <description>The denso robot ros stack constains libraries, configuration files, and ROS nodes for controlling a DENSO robot from ROS.</description>
  <maintainer email="denso_robot_soft@denso-wave.co.jp">DENSO WAVE INCORPORATED</maintainer>
  <license>MIT</license>
  <url type="website">http://ros.org/wiki/denso_robot_ros</url>
  <author email="denso_robot_soft@denso-wave.co.jp">DENSO WAVE INCORPORATED</author>

  <buildtool_depend>catkin</buildtool_depend>
  <run_depend>bcap_core</run_depend>
  <run_depend>bcap_service</run_depend>
  <run_depend>bcap_service_test</run_depend>
  <run_depend>denso_robot_bringup</run_depend>
  <run_depend>denso_robot_control</run_depend>
  <run_depend>denso_robot_core</run_depend>
  <run_depend>denso_robot_core_test</run_depend>
  <run_depend>denso_robot_descriptions</run_depend>
  <run_depend>denso_robot_gazebo</run_depend>
  <run_depend>denso_robot_moveit_config</run_depend>

  <export>
    <metapackage />
  </export>
</package>
